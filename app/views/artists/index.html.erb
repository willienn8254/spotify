
<div class="container">

	<h1>Artistas</h1>
	  <div class="row">


	  	<%  @artists.each do |artist| %>


	  	<div class="col-sm-6 col-md-3">
	  	    <div class="thumbnail">
	  	      <img src="<%= artist.image_url %>" alt="<%= artist.name %>">
	  	      <div class="caption">
	  	        <h3><%= artist.name %></h3>
	  	        <p><a href="#" class="btn btn-primary" role="button">Ver mas</a></p>
	  	      </div>
	  	    </div>
	  	  </div>
	   
		<% end %>

	   </div>
</div>